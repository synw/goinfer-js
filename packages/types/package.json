    "tslib": "^2.6.2",
    "typedoc": "^0.25.1",
    "typedoc-plugin-markdown": "^3.15.4",
    "typedoc-plugin-rename-defaults": "^0.6.5",
    "typescript": "^4.9.5"
  },
  "type": "module",
